{
  "name": "redux-normalized-entities",
  "version": "0.0.3",
  "description": "Store and retrieve data easily",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "dependencies": {
    "redux": "3.6.0",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-istanbul": "3.0.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "deep-freeze": "0.0.1",
    "expect": "1.20.2",
    "ghooks": "1.3.2",
    "mocha": "3.1.2",
    "npm-run-all": "3.1.1",
    "nyc": "10.0.0",
    "rimraf": "2.5.4",
    "validate-commit-msg": "2.8.2",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  },
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --ignore '**/*.spec.js'",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore '**/*.spec.js'",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack ./src/index.js ./dist/redux-normalized-entities.js",
    "build:umd.min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack ./src/index.js ./dist/redux-normalized-entities.min.js",
    "prebuild": "rimraf lib dist es",
    "build": "npm-run-all --parallel build:*",
    "coverage": "cross-env BABEL_ENV=test NODE_ENV=test nyc npm test",
    "check-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha --compilers js:babel-register --recursive ./src/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcollis/redux-normalized-entities.git"
  },
  "keywords": [
    "redux",
    "react",
    "normalizr",
    "reducer"
  ],
  "author": "Mike Collis <collis.michael@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcollis/redux-normalized-entities/issues"
  },
  "homepage": "https://github.com/mcollis/redux-normalized-entities#readme",
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run coverage && npm run check-coverage",
      "pre-push": "npm run coverage && npm run check-coverage",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
